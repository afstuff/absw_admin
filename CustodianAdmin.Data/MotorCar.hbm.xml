<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="CustodianAdmin.Model" namespace="CustodianAdmin.Model">
  <class name="MotorCar" table="ABSMOTCARS">
    <id name="mId" column="ID">
      <generator class="identity"  />

    </id>
    <property name="Make" column="Car_Make" length="50" />
    <property name="Type" column="Car_Type" length="50" />
    <property name="Flag" column="Car_Flag" length="1" />
    <property name="EntryDate" column="Car_KeyDte" type="datetime" />
    <property name="OperatorId" column="Car_OperID" length="10" />

  </class>
</hibernate-mapping>

