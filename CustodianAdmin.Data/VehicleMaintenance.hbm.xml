    <property name="Description" column="ADM_VEH_MAINT_TRANS_DESC" length="150" />
    <property name="ServiceCompany" column="ADM_VEH_MAINT_SERVICE_COY" length="50" />
    <property name="EntryFlag" column="ADM_VEH_MAINT_ENTRY_FLAG" length="003" />
    <property name="EntryDate" column="ADM_VEH_MAINT_ENTRY_DATE" type="datetime" />
    <property name="OperatorId" column="ADM_VEH_MAINT_OPER_ID" length="5" />
  </class>
</hibernate-mapping>
