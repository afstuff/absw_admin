<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="CustodianAdmin.Model" namespace="CustodianAdmin.Model">
  <class name="Department" table="ABSDEPTTAB">
    <id name="dId" column="CTINSRD_NO">
      <generator class="identity"  />
    </id>
    <property name="DeptID" column="CTINSRD_ID" length="3" />
    <property name="DeptNo" column="CTINSRD_NUM" length="10" />
    <property name="LongDesc" column="CTINSRD_LONG_DESCR" length="50" />
    <property name="ShortDesc" column="CTINSRD_SHORT_DESCR" length="20" />
    <property name="Flag" column="CTINSRD_FLAG" length="1" />
    <property name="EntryDate" column="CTINSRD_KEYDTE" type="datetime" />
    <property name="OperatorId" column="CTINSRD_OPERID" length="6" />
  </class>
  
</hibernate-mapping>

