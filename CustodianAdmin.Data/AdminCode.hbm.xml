<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="CustodianAdmin.Model" namespace="CustodianAdmin.Model">
  <class name="AdminCode" table="ADM_MAINT_CODE_TABLE">
    <id name="icId" column="ADM_TAB_REC_ID">
      <generator class="identity"  />
    </id>
    <property name="ClassCode" column="ADM_TAB_CLASS_CODE" length="10" />
    <property name="ItemCode" column="ADM_TAB_ITEM_CODE_NO" length="15" />
    <property name="ItemDesc" column="ADM_TAB_ITEM_DESC" length="100" />
    <property name="TransType" column="ADM_COD_TRANS_TYPE" length="10" />
    <property name="Branch" column="ADM_TAB_ITEM_BRANCH" length="5" />
    <property name="Dept" column="ADM_TAB_ITEM_DEPT" length="5" />
    <property name="Flag" column="ADM_TAB_ITEM_FLAG" length="3" />
    <property name="EntryDate" column="ADM_TAB_ITEM_DATE" type="datetime" />
    <property name="OperatorId" column="ADM_TAB_ITEM_OPER_ID" length="5" />
  </class>
</hibernate-mapping>



